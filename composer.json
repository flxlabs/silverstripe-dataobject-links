{
  "name": "flxlabs/silverstripe-dataobject-links",
  "description": "Adds links to data objects in TinyMCE editor",
  "type": "silverstripe-vendormodule",
  "keywords": [
    "silverstripe",
    "cms",
    "tinymce"
  ],
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "Marco Crespi",
      "email": "mrc@flxlabs.com"
    }
  ],
  "require": {
    "silverstripe/framework": "^6",
    "silverstripe/admin": "^3.1"
  },
  "autoload": {
    "psr-4": {
      "FLXLabs\\DataObjectLink\\": "src/"
    }
  },
  "extra": {
    "expose": [
      "client/dist"
    ]
  },
  "prefer-stable": true,
  "minimum-stability": "stable",
  "config": {
    "allow-plugins": {
      "composer/installers": true,
      "silverstripe/vendor-plugin": true
    }
  }
}
