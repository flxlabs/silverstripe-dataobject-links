{
  "name": "silverstripe-dataobject-links",
  "version": "1.1.0",
  "description": "Adds links to data objects in TinyMCE editor",
  "author": "FLXLabs",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "npm i && NODE_ENV=production webpack -p --bail --progress",
    "dev": "NODE_ENV=development webpack --progress",
    "watch": "NODE_ENV=development webpack --watch --progress",
    "css": "WEBPACK_CHILD=css npm run build",
    "lint": "eslint client/src && sass-lint -v"
  },
  "resolutions": {
    "eslint": "4.6.1"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ]
  },
  "devDependencies": {
    "@silverstripe/eslint-config": "0.0.5",
    "@silverstripe/webpack-config": "1.1.0",
    "webpack": "4.39.2",
    "webpack-cli": "3.3.7"
  },
  "dependencies": {
    "babel-loader": "7.1.5",
    "babel-polyfill": "6.26.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "deep-freeze-strict": "1.1.1",
    "graphql-tag": "2.10.1",
    "jquery": "3.4.1",
    "react": "16.9.0",
    "react-apollo": "3.0.1",
    "react-dom": "16.9.0",
    "react-redux": "7.1.0",
    "react-select": "3.0.4",
    "reactstrap": "8.0.1",
    "redux": "4.0.4",
    "redux-form": "8.2.6"
  }
}
